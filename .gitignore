# .gitignore for PHP

# Core
vendor/
composer.lock
/storage/*
!/storage/app
!/storage/framework
!/storage/logs

# Environment files
.env
*.env.local
*.env.*.local

# Directories generated by frameworks
/includes
/bootstrap/cache
/public/uploads
/public/files
/public/assets

# Session files
/sessions

# Logs
/*.log
/error_log

# Cache
/cache/*
!/cache/.*
/tmp/

# Database
*.sqlite
*.sqlite3

# IDE specific
.idea/
.vscode/
*.swp
*~
nbproject/

# OS generated files
.DS_Store
Thumbs.db

# Composer cache
/composer.phar

# Package specific
node_modules/
package-lock.json
yarn.lock

# Docker
docker-compose.override.yml
